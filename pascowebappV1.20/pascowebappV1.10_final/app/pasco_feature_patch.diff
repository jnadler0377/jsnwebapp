+from fastapi import FastAPI, Request, Depends, UploadFile, File, Form, Query
+from fastapi.responses import HTMLResponse, RedirectResponse
+from fastapi.staticfiles import StaticFiles
+from fastapi.templating import Jinja2Templates
+from sqlalchemy.orm import Session
+from typing import Optional, List
+import pandas as pd
+import os, shutil, re
+
+from .database import Base, engine, SessionLocal
+from .models import Case, Defendant, Docket
+from .utils import ensure_case_folder, compute_offer_70
+
+app = FastAPI(title="JSN Holdings Foreclosure Manager")
+
+# ---- Static & Uploads -----------------------------------------------------
+from pathlib import Path
+BASE_DIR = Path(__file__).resolve().parent.parent  # root that contains 'app/'
+STATIC_DIR = BASE_DIR / "app" / "static"
+UPLOAD_ROOT = BASE_DIR / "uploads"
+UPLOAD_ROOT.mkdir(parents=True, exist_ok=True)
+
+app.mount("/static", StaticFiles(directory=str(STATIC_DIR)), name="static")
+app.mount("/uploads", StaticFiles(directory=str(UPLOAD_ROOT)), name="uploads")
+templates = Jinja2Templates(directory=str(BASE_DIR / "app" / "templates"))
+
+# ---- DB bootstrap & lightweight migration for new columns -----------------
+Base.metadata.create_all(bind=engine)
+from sqlalchemy import text
+def _ensure_column(table: str, column: str, type_sql: str, default_sql: str = "DEFAULT 0"):
+    with engine.begin() as conn:
+        cols = [r[1] for r in conn.execute(text(f"PRAGMA table_info({table})")).fetchall()]
+        if column not in cols:
+            conn.execute(text(f"ALTER TABLE {table} ADD COLUMN {column} {type_sql} {default_sql}"))
+
+try:
+    _ensure_column("cases", "address_override", "TEXT", "DEFAULT ''")
+    _ensure_column("cases", "arv", "REAL")
+    _ensure_column("cases", "rehab_estimate", "REAL")
+    _ensure_column("cases", "closing_costs", "REAL")
+    _ensure_column("cases", "offer_70", "REAL")
+except Exception:
+    pass
+
+# ---- Utilities exposed to Jinja -------------------------------------------
+def parcel_url(parcel_id: str) -> Optional[str]:
+    \"\"\"
+    Convert a parcel id like '25-26-15-0060-00000-0470' to the Pasco appraiser search URL.
+    \"\"\"
+    if not parcel_id:
+        return None
+    m = re.match(r"^(\d{2})-(\d{2})-(\d{2})-(\d{4})-(\d{5})-(\d{4})$", parcel_id.strip())
+    if not m:
+        return None
+    sec, twn, rng, sbb, blk, lot = m.groups()
+    return f"https://search.pascopa.com/parcel.aspx?sec={sec}&twn={twn}&rng={rng}&sbb={sbb}&blk={blk}&lot={lot}&action=Submit"
+
+templates.env.globals["parcel_url"] = parcel_url
+
+# ---- Dependency ------------------------------------------------------------
+def get_db():
+    db = SessionLocal()
+    try:
+        yield db
+    finally:
+        db.close()
+
+# ---- Routes: Cases list with filters --------------------------------------
+@app.get("/cases", response_class=HTMLResponse)
+def cases_list(
+    request: Request,
+    q: Optional[str] = Query(None, description="Search across case #, style, address, parcel id"),
+    defendant: Optional[str] = Query(None),
+    filed_from: Optional[str] = Query(None, description="Inclusive YYYY-MM-DD"),
+    filed_to: Optional[str] = Query(None, description="Inclusive YYYY-MM-DD"),
+    has_docs: Optional[bool] = Query(False, description="Only cases with any uploaded docs"),
+    db: Session = Depends(get_db),
+):
+    query = db.query(Case)
+    if q:
+        like = f"%{q.strip()}%"
+        query = query.filter(
+            (Case.case_number.like(like)) |
+            (Case.style.like(like)) |
+            (Case.address.like(like)) |
+            (Case.address_override.like(like)) |
+            (Case.parcel_id.like(like))
+        )
+    if defendant:
+        d_like = f"%{defendant.strip()}%"
+        query = query.join(Defendant).filter(Defendant.name.like(d_like))
+    if filed_from:
+        query = query.filter(Case.filing_datetime >= filed_from)
+    if filed_to:
+        query = query.filter(Case.filing_datetime <= filed_to)
+    if has_docs:
+        query = query.filter(
+            (Case.verified_complaint_path != "") |
+            (Case.value_calc_path != "") |
+            (Case.mortgage_path != "")
+        )
+    cases = query.order_by(Case.filing_datetime.desc()).limit(500).all()
+    return templates.TemplateResponse("cases_list.html", {"request": request, "cases": cases})
+
+# ---- Route: Case detail update incl. parcel/address & offer calculator -----
+@app.post("/cases/{case_id}/update")
+async def update_case(
+    case_id: int,
+    parcel_id: Optional[str] = Form(None),
+    address_override: Optional[str] = Form(None),
+    arv: Optional[float] = Form(0.0),
+    rehab_estimate: Optional[float] = Form(0.0),
+    closing_costs: Optional[float] = Form(0.0),
+    db: Session = Depends(get_db),
+):
+    case = db.query(Case).filter(Case.id == case_id).first()
+    if not case:
+        return RedirectResponse("/cases", status_code=303)
+
+    if parcel_id is not None:
+        case.parcel_id = parcel_id.strip()
+    if address_override is not None:
+        case.address_override = address_override.strip()
+    try:
+        case.arv = float(arv or 0)
+        case.rehab_estimate = float(rehab_estimate or 0)
+        case.closing_costs = float(closing_costs or 0)
+        case.offer_70 = compute_offer_70(case.arv, case.rehab_estimate, case.closing_costs)
+    except Exception:
+        pass
+
+    db.commit()
+    return RedirectResponse(f"/cases/{case_id}", status_code=303)
+
+# ---- CSV Import UI ---------------------------------------------------------
+@app.get("/import", response_class=HTMLResponse)
+def import_get(request: Request):
+    return templates.TemplateResponse("import.html", {"request": request})
+
+@app.post("/import", response_class=HTMLResponse)
+async def import_post(request: Request, file: UploadFile = File(...), db: Session = Depends(get_db)):
+    import pandas as pd
+    df = pd.read_csv(file.file)
+    def get(df, names: List[str]):
+        cols = {c.lower(): c for c in df.columns}
+        for n in names:
+            if n.lower() in cols:
+                return cols[n.lower()]
+        return None
+    c_case = get(df, ["Case #", "Case#", "Case Number", "Case No"])
+    c_name = get(df, ["Case Name", "Style", "Case Style"])
+    c_file = get(df, ["Filing Date", "Filing", "Filed"])
+    def_cols = [get(df, [f"Defendant {i}", f"Def {i}", f"Defendant{i}"]) for i in range(1, 20)]
+    def_cols = [c for c in def_cols if c]
+
+    created = 0; updated = 0; added_defendants = 0
+    for _, row in df.iterrows():
+        case_number = str(row.get(c_case, "")).strip().replace("\\", "-").replace("/", "-")
+        if not case_number:
+            continue
+        style = str(row.get(c_name, "") or "").strip()
+        filing = str(row.get(c_file, "") or "").strip()
+        case = db.query(Case).filter(Case.case_number == case_number).first()
+        if not case:
+            case = Case(case_number=case_number, style=style, filing_datetime=filing)
+            db.add(case); db.flush(); created += 1
+        else:
+            ch = False
+            if style and not (case.style or "").strip(): case.style = style; ch = True
+            if filing and not (case.filing_datetime or "").strip(): case.filing_datetime = filing; ch = True
+            if ch: updated += 1
+        existing = { (d.name or "").strip().lower() for d in case.defendants }
+        for c in def_cols:
+            name = str(row.get(c, "") or "").strip()
+            if name and name.lower() not in existing:
+                db.add(Defendant(case_id=case.id, name=name)); added_defendants += 1
+    db.commit()
+    return templates.TemplateResponse("import.html", {"request": request, "message": f"Imported: {created} created, {updated} updated, {added_defendants} defendants added."})
